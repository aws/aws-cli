{
    "pagination": {
        "ListMultipartUploads": {
            "limit_key": "MaxUploads",
            "more_results": "IsTruncated",
            "output_token": ["NextKeyMarker", "NextUploadIdMarker"],
            "input_token": ["KeyMarker", "UploadIdMarker"],
            "result_key": "Uploads"
        },
        "ListObjectVersions": {
            "more_results": "IsTruncated",
            "limit_key": "MaxKeys",
            "output_token": ["NextKeyMarker",
                             "NextVersionIdMarker"],
            "input_token": ["KeyMarker", "VersionIdMarker"],
            "result_key": "Versions"
        },
        "ListObjects": {
            "more_results": "IsTruncated",
            "limit_key": "MaxKeys",
            "output_token": "NextMarker or Contents[-1].Key",
            "input_token": "Marker",
            "result_key": ["Contents", "CommonPrefixes"]
        },
        "ListParts": {
            "limit_key": "IsTruncated",
            "output_tokens": "NextPartNumberMarker",
            "input_token": "PartNumberMarker",
            "result_key": "Parts"
        }
    }
}
